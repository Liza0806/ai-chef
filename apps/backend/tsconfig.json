{
  "extends": ["../../apps/tsconfig.json", "./tsconfig.paths.json"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
    "@recipes/*": ["recipes/*"],
    "@upload/*": ["upload/*"],
    "@prisma/*": ["prisma/*"],
    "@recognize/*": ["recognize/*"]
    },
    "types": ["node", "jest"],
    "target": "ES2020",
"module": "CommonJS",
"moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "dist"
  },
  "include": ["src/**/*.ts", "tests/**/*.ts", "jest.config.ts", "prisma/seed.ts", "globals.d.ts"]
}
